package test5;

//Mean and Median of an Array

public class Question84 
{
	public static void main(String[] args) {
		int arr[]= {1,4,5,7,8,9};

		int n=arr.length;
		
		meanofelements(arr,n);
	}

	private static void meanofelements(int[] arr, int n) 
	{
		int sum=0;
		
		for(int i:arr)
		{
			sum=sum+i;
		}
		
		int result= sum/n;
		
		
	}
	
	
	
}
