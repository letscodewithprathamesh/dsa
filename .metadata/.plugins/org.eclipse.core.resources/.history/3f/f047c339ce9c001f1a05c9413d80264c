package test5;

//Seperate Even and Odd Number

public class Question82 
{
	public static void main(String[] args)
	{
		int arr[]= {3,5,7,2,8,6};
		
		
		
		for(int i=0;i<arr.length;i++)
		{
			for(int j=i+1;j<arr.length;j++)
			{
				if(arr[i]%2 == 0)
				{
					int temp=arr[i];
					arr[i]=arr[j];
					arr[j]=temp;
				}
			}
		}
		
		for(int out:arr)
		{
			System.out.print(out);
		}
		
	}

}
