package test4;

//Toggle the String

public class Question68 
{
	public static void main(String[] args) 
	{
		String s="NaNa";
		
		char ch[]=s.toCharArray();
		
		String result="";
		
		for(int i=0;i<ch.length;i++)
		{
			if(ch[i]>='A' && ch[i]<='Z')
			{
				result=result+(ch[i]+32);
			}
			else if(ch[i]>='a' && ch[i]<='z')
			{
				result=result+(ch[i]-32);
			}
			
		}
		
		System.out.println(result);
		
		
	}

}
