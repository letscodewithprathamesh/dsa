package test5;

//Find Common elements from 2 Array

public class Question85 {
	public static void main(String[] args) {
		
		int arr1[]= {1,2,3,4,5};
		int arr2[]= {4,5,6,7,8,9};
		
		commonElements(arr1,arr2);
	}

	private static void commonElements(int[] arr1, int[] arr2) {
		
		for(int i:arr1)
		{
			for(int j:arr2)
			{
				if(i == j)
				{
					System.out.print(i+",");
				}
			}
		}
		
		
	}

}
