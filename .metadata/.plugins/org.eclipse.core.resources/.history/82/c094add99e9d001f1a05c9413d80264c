package test1;

//Decimal to  Binary

public class Question23
{
	public static void main(String[] args) {
		
		int num=12;
		
		int i=0;
		
		int binary[]=new int[100];
		
		while(num != 0)
		{
			binary[i]=num%2;
			i++;
			num=num/2;
		}
		
		for(int j=i-1;j>=0;j--)
		{
			System.out.print(binary[j]);
		}
		
		
	}

}
