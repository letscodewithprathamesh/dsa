package test4;

//Sorting String Alphabetically

public class Question58 
{
	public static void main(String[] args)
	{
		String s="kbc";
		
		String temp="";
		
		char ch[]=s.toCharArray();
		
		for(int i=0;i<ch.length;i++)
		{
			for(int j=i+1;j<ch.length;j++)
			{
				if(ch[i]>ch[j])
				{
					temp=ch[i]+"";
					ch[i]=ch[j];
					ch[j]=temp+"";
				}
			}
		}
		
		
	}
}


